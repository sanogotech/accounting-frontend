{
	"name": "accounting-web",
	"private": true,
	"version": "0.0.0",
	"description": "Front End accounting tracker",
	"repository": {
		"type": "git",
		"url": "https://github.com/tortinez/accounting-frontend.git"
	},
	"license": "MIT",
	"author": "Victor Martinez",
	"dependencies": {},
	"devDependencies": {
		"bower": "^1.7.7",
		"http-server": "^0.9.0",
		"jasmine-core": "^2.4.1",
		"karma": "^0.13.22",
		"karma-chrome-launcher": "^0.2.3",
		"karma-firefox-launcher": "^0.1.7",
		"karma-jasmine": "^0.3.8",
		"protractor": "^4.0.9"
	},
	"scripts": {
		"prebower-patch": "bower install",
		"bower-patch":
			"patch -N ./src/bower_components/md-color-menu/md-color-menu.js < ./md-color-menu.patch.js",
		"postinstall": "npm run bower-patch",
		"install": "npm install",
		"start":
			"http-server ./src -a localhost -p 8000 -c-1 -P http://localhost:8080",
		"pretest": "npm install",
		"test": "karma start karma.conf.js",
		"test-single-run": "karma start karma.conf.js --single-run",
		"preupdate-webdriver": "npm install",
		"update-webdriver": "webdriver-manager update",
		"preprotractor": "npm run update-webdriver",
		"protractor": "protractor e2e-tests/protractor.conf.js"
	}
}
