		"style-loader": "^0.13.0",
		"webpack": "3.11.0",
		"webpack-dev-server": "2.2.0"
	},
	"scripts": {
		"build": "rimraf dist && webpack --bail --progress --profile",
		"server": "webpack-dev-server --history-api-fallback --inline --progress --host 0.0.0.0",
		"start": "npm run server",
		"md-color-menu-patch": "patch -N ./node_modules/md-color-menu/md-color-menu.js < ./md-color-menu.patch.js",
		"postinstall": "npm run md-color-menu-patch",
		"pretest": "npm install",
		"test": "karma start",
		"test-watch": "karma start --auto-watch --no-single-run",
		"test-single-run": "karma start karma.conf.js --single-run",
		"preupdate-webdriver": "npm install",
		"update-webdriver": "webdriver-manager update",
		"preprotractor": "npm run update-webdriver",
		"protractor": "protractor e2e-tests/protractor.conf.js"
	}
}
