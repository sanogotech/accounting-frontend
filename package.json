	"devDependencies": {
		"angular-mocks": "^1.5.0",
		"autoprefixer": "^6.0.3",
		"babel-core": "^6.2.1",
		"babel-loader": "^6.2.0",
		"babel-preset-es2015": "^6.1.18",
		"copy-webpack-plugin": "4.0.1",
		"css-loader": "0.26.1",
		"extract-text-webpack-plugin": "2.0.0-beta.5",
